{
  "@context": {
    "@version": 1.1,
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "ev": "./ev#"
  },
  "@graph": [
    {
      "@id": "ev:connectorType",
      "@type": "rdf:Property",
      "rdfs:label": "Connector type",
      "rdfs:comment": "Physical connector type supported by the EVSE/charger (e.g., CCS2, Type2, CHAdeMO, GB/T).",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:maxPowerKW",
      "@type": "rdf:Property",
      "rdfs:label": "Maximum power (kW)",
      "rdfs:comment": "Maximum charging power supported by the EVSE in kilowatts.",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:minPowerKW",
      "@type": "rdf:Property",
      "rdfs:label": "Minimum power (kW)",
      "rdfs:comment": "Minimum effective charging power in kilowatts.",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:socketCount",
      "@type": "rdf:Property",
      "rdfs:label": "Socket count",
      "rdfs:comment": "Number of simultaneously usable sockets/connectors.",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Integer"
    },
    {
      "@id": "ev:reservationSupported",
      "@type": "rdf:Property",
      "rdfs:label": "Reservation supported",
      "rdfs:comment": "Whether reservation of a time window/slot is supported.",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Boolean"
    },
    {
      "@id": "ev:ocppId",
      "@type": "rdf:Property",
      "rdfs:label": "OCPP identifier",
      "rdfs:comment": "Opaque internal identifier used by the CPO/OCPP backend.",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:evseId",
      "@type": "rdf:Property",
      "rdfs:label": "EVSE identifier",
      "rdfs:comment": "Public EVSE identifier (if exposed).",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:tariffModel",
      "@type": "rdf:Property",
      "rdfs:label": "Tariff model",
      "rdfs:comment": "The charging tariff basis: PER_KWH, PER_MINUTE, SUBSCRIPTION, TIME_OF_DAY.",
      "schema:domainIncludes": "schema:Offer",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:supportedUnits",
      "@type": "rdf:Property",
      "rdfs:label": "Supported units",
      "rdfs:comment": "Units accepted for pricing/measurement (e.g., kWh, minute, hour, month).",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:meteredEnergyKWh",
      "@type": "rdf:Property",
      "rdfs:label": "Metered energy (kWh)",
      "rdfs:comment": "Energy dispensed during a charging session, in kilowatt-hours.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:meteredDurationMinutes",
      "@type": "rdf:Property",
      "rdfs:label": "Metered duration (minutes)",
      "rdfs:comment": "Duration of the charging session, in minutes.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:roamingNetwork",
      "@type": "rdf:Property",
      "rdfs:label": "Roaming network",
      "rdfs:comment": "Roaming federation or network, if any.",
      "schema:domainIncludes": "schema:Organization",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:parkingType",
      "@type": "rdf:Property",
      "rdfs:label": "Parking type",
      "rdfs:comment": "Type of parking facility (e.g., Mall, Office, FuelStation).",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:operatorCode",
      "@type": "rdf:Property",
      "rdfs:label": "Operator code",
      "rdfs:comment": "Short operator code used in roaming/indexes.",
      "schema:domainIncludes": "schema:Organization",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:gstNumber",
      "@type": "rdf:Property",
      "rdfs:label": "GST Number",
      "rdfs:comment": "Tax identifier (India).",
      "schema:domainIncludes": "schema:Organization",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:cin",
      "@type": "rdf:Property",
      "rdfs:label": "CIN",
      "rdfs:comment": "Corporate Identification Number (India).",
      "schema:domainIncludes": "schema:Organization",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:msme",
      "@type": "rdf:Property",
      "rdfs:label": "MSME ID",
      "rdfs:comment": "MSME registration identifier (India).",
      "schema:domainIncludes": "schema:Organization",
      "schema:rangeIncludes": "schema:Text"
    },

    /* ---------- NEW connector/offer/status terms ---------- */
    {
      "@id": "ev:connectorId",
      "@type": "rdf:Property",
      "rdfs:label": "Connector ID",
      "rdfs:comment": "Provider-specific connector or socket identifier.",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:powerType",
      "@type": "rdf:Property",
      "rdfs:label": "Power type",
      "rdfs:comment": "Electrical power type (e.g., AC_SINGLE_PHASE, AC_3_PHASE, DC).",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:connectorFormat",
      "@type": "rdf:Property",
      "rdfs:label": "Connector format",
      "rdfs:comment": "Physical presentation of the connector (SOCKET or CABLE).",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:chargingSpeed",
      "@type": "rdf:Property",
      "rdfs:label": "Charging speed band",
      "rdfs:comment": "Marketing speed band (SLOW, NORMAL, FAST, ULTRAFAST).",
      "schema:domainIncludes": "schema:Service",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:stationStatus",
      "@type": "rdf:Property",
      "rdfs:label": "Station status",
      "rdfs:comment": "Real-time availability status of the station or connector.",
      "schema:domainIncludes": [
        "schema:Place",
        "schema:Service"
      ],
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:idleFeePolicy",
      "@type": "rdf:Property",
      "rdfs:label": "Idle fee policy",
      "rdfs:comment": "Human-readable policy for post-charge idle fees associated with an offer.",
      "schema:domainIncludes": "schema:Offer",
      "schema:rangeIncludes": "schema:Text"
    },

    /* ---------- NEW session auth & tracking ---------- */
    {
      "@id": "ev:authorizationMode",
      "@type": "rdf:Property",
      "rdfs:label": "Authorization mode",
      "rdfs:comment": "Mode used to authorize/start the session (e.g., APP_QR, RFID, OTP).",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:authorizationOtpHint",
      "@type": "rdf:Property",
      "rdfs:label": "Authorization OTP hint",
      "rdfs:comment": "Optional OTP or hint for kiosk entry during session start.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:trackingId",
      "@type": "rdf:Property",
      "rdfs:label": "Tracking ID",
      "rdfs:comment": "Provider tracking identifier for this session.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:trackingUrl",
      "@type": "rdf:Property",
      "rdfs:label": "Tracking URL",
      "rdfs:comment": "Live session dashboard URL.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:URL"
    },
    {
      "@id": "ev:trackingStatus",
      "@type": "rdf:Property",
      "rdfs:label": "Tracking status",
      "rdfs:comment": "Current tracking state (e.g., ACTIVE, INACTIVE).",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Text"
    },

    /* ---------- NEW OCPI-like telemetry and totals ---------- */
    {
      "@id": "ev:kwh",
      "@type": "rdf:Property",
      "rdfs:label": "Energy (kWh)",
      "rdfs:comment": "Total energy consumed during the session in kilowatt-hours.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:chargingPeriods",
      "@type": "rdf:Property",
      "rdfs:label": "Charging periods",
      "rdfs:comment": "List of charging intervals with telemetry dimensions.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:ItemList"
    },
    {
      "@id": "ev:dimensions",
      "@type": "rdf:Property",
      "rdfs:label": "Dimensions",
      "rdfs:comment": "Telemetry dimensions recorded in a period (e.g., ENERGY, POWER, SOC).",
      "schema:domainIncludes": "schema:ListItem",
      "schema:rangeIncludes": "schema:ItemList"
    },
    {
      "@id": "ev:dimensionType",
      "@type": "rdf:Property",
      "rdfs:label": "Dimension type",
      "rdfs:comment": "Type of telemetry dimension (e.g., ENERGY, POWER, CURRENT, VOLTAGE, STATE_OF_CHARGE).",
      "schema:domainIncludes": "schema:ListItem",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:volume",
      "@type": "rdf:Property",
      "rdfs:label": "Dimension volume/value",
      "rdfs:comment": "Numeric value for the dimension (units depend on the type).",
      "schema:domainIncludes": "schema:ListItem",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:totalCost",
      "@type": "rdf:Property",
      "rdfs:label": "Total cost",
      "rdfs:comment": "Total session cost as a price specification.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:PriceSpecification"
    },
    {
      "@id": "ev:exclVat",
      "@type": "rdf:Property",
      "rdfs:label": "Excl. VAT amount",
      "rdfs:comment": "Total amount excluding VAT.",
      "schema:domainIncludes": "schema:PriceSpecification",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:inclVat",
      "@type": "rdf:Property",
      "rdfs:label": "Incl. VAT amount",
      "rdfs:comment": "Total amount including VAT.",
      "schema:domainIncludes": "schema:PriceSpecification",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:sessionStatus",
      "@type": "rdf:Property",
      "rdfs:label": "Session status",
      "rdfs:comment": "High-level session state (e.g., PENDING, ACTIVE, COMPLETED, INTERRUPTED).",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Text"
    },

    /* ---------- NEW buyer-finder fee ---------- */
    {
      "@id": "ev:buyerFinderFee",
      "@type": "rdf:Property",
      "rdfs:label": "Buyer finder fee",
      "rdfs:comment": "Commission payable (percentage or absolute) associated with an offer or order.",
      "schema:domainIncludes": [
        "schema:Offer",
        "schema:Order"
      ],
      "schema:rangeIncludes": "schema:PriceSpecification"
    },
    {
      "@id": "ev:feeType",
      "@type": "rdf:Property",
      "rdfs:label": "Fee type",
      "rdfs:comment": "Type of fee (e.g., PERCENTAGE, AMOUNT).",
      "schema:domainIncludes": "schema:PriceSpecification",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:feeValue",
      "@type": "rdf:Property",
      "rdfs:label": "Fee value",
      "rdfs:comment": "Numeric fee value (interpreted per fee type).",
      "schema:domainIncludes": "schema:PriceSpecification",
      "schema:rangeIncludes": "schema:Number"
    },

    /* ---------- OPTIONAL vehicle hints (if you map them to ev:) ---------- */
    {
      "@id": "ev:vehicleMake",
      "@type": "rdf:Property",
      "rdfs:label": "Vehicle make",
      "rdfs:comment": "Vehicle make/brand recorded for the session.",
      "schema:domainIncludes": [
        "schema:Vehicle",
        "schema:Order"
      ],
      "schema:rangeIncludes": [
        "schema:Brand",
        "schema:Text"
      ]
    },
    {
      "@id": "ev:vehicleModel",
      "@type": "rdf:Property",
      "rdfs:label": "Vehicle model",
      "rdfs:comment": "Vehicle model recorded for the session.",
      "schema:domainIncludes": [
        "schema:Vehicle",
        "schema:Order"
      ],
      "schema:rangeIncludes": "schema:Text"
    },

    {
      "@id": "ev:reservationId",
      "@type": "rdf:Property",
      "rdfs:label": "Reservation ID",
      "rdfs:comment": "Unique identifier for a reserved charging slot or booking.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Text"
    },
    {
      "@id": "ev:gracePeriodMinutes",
      "@type": "rdf:Property",
      "rdfs:label": "Grace period (minutes)",
      "rdfs:comment": "Grace period in minutes before idle fee applies or reservation is released.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Integer"
    },
    {
      "@id": "ev:ratingOverall",
      "@type": "rdf:Property",
      "rdfs:label": "Overall rating",
      "rdfs:comment": "User's overall rating (1–5) for the session.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Number"
    },
    {
      "@id": "ev:ratingAmenities",
      "@type": "rdf:Property",
      "rdfs:label": "Amenities rating",
      "rdfs:comment": "User's rating (1–5) for on-site amenities.",
      "schema:domainIncludes": "schema:Order",
      "schema:rangeIncludes": "schema:Number"
    }
  ]
}